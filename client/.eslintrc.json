{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true,
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-unused-vars": ["warn", { "args": "none" }],
        "react/prop-types": 0
    }
}
